    <TIER ANNOTATOR="sofyafedorenko" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="@vfk1918f-text"
        PARTICIPANT="@vfk1918f" TIER_ID="transl@vfk1918f">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a28" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>My life, my childhood...It was hard. I am an orphan. My father left for the front when I was six week old.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a7" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>In 1918. In six months he missed. 18-year-old Mom became a widow. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a29" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>What?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a30" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>Yes</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a31" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>Yes. In 1918</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a15" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>Somewhere...With Chapaev. I can not say exactly. Near Levinka of Dyhovnickiy area. So...Firstly, I lived with my mother. Of course, she was young and wanted to walk. So, I spent most time with my grandmother. And our grandfather was eternal fisher. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a39" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>Yes, yes, yes, yes. Gusev Trofim Evstahich</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a40" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>Grandfather. My mother's father.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a41" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>Mine father?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a44" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>Fedor. I am Fedorovna.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a47" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>Perevozchikov</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
